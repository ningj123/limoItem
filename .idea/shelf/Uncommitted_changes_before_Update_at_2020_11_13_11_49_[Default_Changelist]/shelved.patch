r\n    public void updateStatus(JoinParam joinParam) throws Exception {\r\n        LimoJoin limoJoin = new LimoJoin();\r\n        BeanUtils.copyProperties(joinParam,limoJoin);\r\n        limoJoinMapper.updateById(limoJoin);\r\n    }\r\n}\r\n
